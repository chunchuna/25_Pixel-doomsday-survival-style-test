						"content": "<!DOCTYPE html>\n<html lang=\"zh-CN\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>CHUN CHUN STUDIO 光球动画</title>\n    <style>\n        body {\n            margin: 0;\n            height: 100vh;\n            width: 100vw;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            background-color: #000;\n            background-image: \n                linear-gradient(rgba(0, 255, 255, 0.1) 1px, transparent 1px),\n                linear-gradient(90deg, rgba(0, 255, 255, 0.1) 1px, transparent 1px);\n            background-size: 30px 30px;\n            overflow: hidden;\n            position: fixed;\n            top: 0;\n            left: 0;\n        }\n        \n        .scene {\n            width: 100vw;\n            height: 100vh;\n            position: relative;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n        }\n        \n        /* 文字光球 */\n        .text-ball {\n            position: absolute;\n            width: 10px;\n            height: 10px;\n            background: radial-gradient(circle, \n                                       rgba(255, 255, 255, 0.9) 0%,\n                                       rgba(93, 240, 255, 0.7) 30%,\n                                       rgba(41, 171, 226, 0.5) 70%,\n                                       transparent 100%);\n            border-radius: 50%;\n            border: 1px solid rgba(93, 240, 255, 0.5);\n            box-shadow: 0 0 20px 10px rgba(93, 240, 255, 0.4),\n                        inset 0 0 8px 3px rgba(255, 255, 255, 0.3);\n            z-index: 5;\n        }\n        \n        /* CHUN 第一行 */\n        .ball-c1 { animation: ball-c1-anim 8s infinite; animation-delay: 0s; }\n        .ball-h1 { animation: ball-h1-anim 8s infinite; animation-delay: 0.1s; }\n        .ball-u1 { animation: ball-u1-anim 8s infinite; animation-delay: 0.2s; }\n        .ball-n1 { animation: ball-n1-anim 8s infinite; animation-delay: 0.3s; }\n        \n        /* 第二个CHUN */\n        .ball-c2 { animation: ball-c2-anim 8s infinite; animation-delay: 0.4s; }\n        .ball-h2 { animation: ball-h2-anim 8s infinite; animation-delay: 0.5s; }\n        .ball-u2 { animation: ball-u2-anim 8s infinite; animation-delay: 0.6s; }\n        .ball-n2 { animation: ball-n2-anim 8s infinite; animation-delay: 0.7s; }\n        \n        /* STUDIO 第二行 */\n        .ball-s { animation: ball-s-anim 8s infinite; animation-delay: 0.8s; }\n        .ball-t { animation: ball-t-anim 8s infinite; animation-delay: 0.9s; }\n        .ball-u3 { animation: ball-u3-anim 8s infinite; animation-delay: 1s; }\n        .ball-d { animation: ball-d-anim 8s infinite; animation-delay: 1.1s; }\n        .ball-i { animation: ball-i-anim 8s infinite; animation-delay: 1.2s; }\n        .ball-o { animation: ball-o-anim 8s infinite; animation-delay: 1.3s; }\n        \n        /* 额外的装饰光球 */\n        .ball-extra1 { animation: ball-extra1-anim 8s infinite; animation-delay: 1.4s; }\n        .ball-extra2 { animation: ball-extra2-anim 8s infinite; animation-delay: 1.5s; }\n        .ball-extra3 { animation: ball-extra3-anim 8s infinite; animation-delay: 1.6s; }\n        .ball-extra4 { animation: ball-extra4-anim 8s infinite; animation-delay: 1.7s; }\n        .ball-extra5 { animation: ball-extra5-anim 8s infinite; animation-delay: 1.8s; }\n        .ball-extra6 { animation: ball-extra6-anim 8s infinite; animation-delay: 1.9s; }\n        \n        /* C 字母动画 */\n        @keyframes ball-c1-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 20%; }\n            62.5% { opacity: 1; top: 40%; left: 20%; }\n            62.6% { opacity: 1; top: 40%; left: 20%; }\n            75% { opacity: 1; top: 15%; left: 75%; }\n            87.5% { opacity: 1; top: 80%; left: 30%; }\n            100% { opacity: 1; top: 40%; left: 20%; }\n        }\n        \n        /* H 字母动画 */\n        @keyframes ball-h1-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 28%; }\n            62.5% { opacity: 1; top: 40%; left: 28%; }\n            62.6% { opacity: 1; top: 40%; left: 28%; }\n            75% { opacity: 1; top: 70%; left: 85%; }\n            87.5% { opacity: 1; top: 25%; left: 15%; }\n            100% { opacity: 1; top: 40%; left: 28%; }\n        }\n        \n        /* U 字母动画 */\n        @keyframes ball-u1-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 36%; }\n            62.5% { opacity: 1; top: 40%; left: 36%; }\n            62.6% { opacity: 1; top: 40%; left: 36%; }\n            75% { opacity: 1; top: 35%; left: 60%; }\n            87.5% { opacity: 1; top: 65%; left: 45%; }\n            100% { opacity: 1; top: 40%; left: 36%; }\n        }\n        \n        /* N 字母动画 */\n        @keyframes ball-n1-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 44%; }\n            62.5% { opacity: 1; top: 40%; left: 44%; }\n            62.6% { opacity: 1; top: 40%; left: 44%; }\n            75% { opacity: 1; top: 20%; left: 40%; }\n            87.5% { opacity: 1; top: 85%; left: 70%; }\n            100% { opacity: 1; top: 40%; left: 44%; }\n        }\n        \n        /* 第二个CHUN */\n        @keyframes ball-c2-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 56%; }\n            62.5% { opacity: 1; top: 40%; left: 56%; }\n            62.6% { opacity: 1; top: 40%; left: 56%; }\n            75% { opacity: 1; top: 60%; left: 20%; }\n            87.5% { opacity: 1; top: 30%; left: 90%; }\n            100% { opacity: 1; top: 40%; left: 56%; }\n        }\n        \n        @keyframes ball-h2-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 64%; }\n            62.5% { opacity: 1; top: 40%; left: 64%; }\n            62.6% { opacity: 1; top: 40%; left: 64%; }\n            75% { opacity: 1; top: 50%; left: 80%; }\n            87.5% { opacity: 1; top: 40%; left: 25%; }\n            100% { opacity: 1; top: 40%; left: 64%; }\n        }\n        \n        @keyframes ball-u2-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 72%; }\n            62.5% { opacity: 1; top: 40%; left: 72%; }\n            62.6% { opacity: 1; top: 40%; left: 72%; }\n            75% { opacity: 1; top: 75%; left: 55%; }\n            87.5% { opacity: 1; top: 15%; left: 35%; }\n            100% { opacity: 1; top: 40%; left: 72%; }\n        }\n        \n        @keyframes ball-n2-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 40%; left: 80%; }\n            62.5% { opacity: 1; top: 40%; left: 80%; }\n            62.6% { opacity: 1; top: 40%; left: 80%; }\n            75% { opacity: 1; top: 25%; left: 65%; }\n            87.5% { opacity: 1; top: 70%; left: 10%; }\n            100% { opacity: 1; top: 40%; left: 80%; }\n        }\n        \n        /* STUDIO 第二行 */\n        @keyframes ball-s-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 60%; left: 25%; }\n            62.5% { opacity: 1; top: 60%; left: 25%; }\n            62.6% { opacity: 1; top: 60%; left: 25%; }\n            75% { opacity: 1; top: 45%; left: 70%; }\n            87.5% { opacity: 1; top: 55%; left: 50%; }\n            100% { opacity: 1; top: 60%; left: 25%; }\n        }\n        \n        @keyframes ball-t-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 60%; left: 33%; }\n            62.5% { opacity: 1; top: 60%; left: 33%; }\n            62.6% { opacity: 1; top: 60%; left: 33%; }\n            75% { opacity: 1; top: 80%; left: 15%; }\n            87.5% { opacity: 1; top: 20%; left: 80%; }\n            100% { opacity: 1; top: 60%; left: 33%; }\n        }\n        \n        @keyframes ball-u3-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 60%; left: 41%; }\n            62.5% { opacity: 1; top: 60%; left: 41%; }\n            62.6% { opacity: 1; top: 60%; left: 41%; }\n            75% { opacity: 1; top: 30%; left: 85%; }\n            87.5% { opacity: 1; top: 75%; left: 40%; }\n            100% { opacity: 1; top: 60%; left: 41%; }\n        }\n        \n        @keyframes ball-d-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 60%; left: 49%; }\n            62.5% { opacity: 1; top: 60%; left: 49%; }\n            62.6% { opacity: 1; top: 60%; left: 49%; }\n            75% { opacity: 1; top: 65%; left: 90%; }\n            87.5% { opacity: 1; top: 35%; left: 60%; }\n            100% { opacity: 1; top: 60%; left: 49%; }\n        }\n        \n        @keyframes ball-i-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 60%; left: 57%; }\n            62.5% { opacity: 1; top: 60%; left: 57%; }\n            62.6% { opacity: 1; top: 60%; left: 57%; }\n            75% { opacity: 1; top: 85%; left: 45%; }\n            87.5% { opacity: 1; top: 10%; left: 75%; }\n            100% { opacity: 1; top: 60%; left: 57%; }\n        }\n        \n        @keyframes ball-o-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 60%; left: 65%; }\n            62.5% { opacity: 1; top: 60%; left: 65%; }\n            62.6% { opacity: 1; top: 60%; left: 65%; }\n            75% { opacity: 1; top: 40%; left: 35%; }\n            87.5% { opacity: 1; top: 90%; left: 85%; }\n            100% { opacity: 1; top: 60%; left: 65%; }\n        }\n        \n        /* 额外装饰光球 */\n        @keyframes ball-extra1-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 25%; left: 15%; }\n            62.5% { opacity: 1; top: 25%; left: 15%; }\n            62.6% { opacity: 1; top: 25%; left: 15%; }\n            75% { opacity: 1; top: 55%; left: 95%; }\n            87.5% { opacity: 1; top: 80%; left: 20%; }\n            100% { opacity: 1; top: 25%; left: 15%; }\n        }\n        \n        @keyframes ball-extra2-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 75%; left: 85%; }\n            62.5% { opacity: 1; top: 75%; left: 85%; }\n            62.6% { opacity: 1; top: 75%; left: 85%; }\n            75% { opacity: 1; top: 20%; left: 50%; }\n            87.5% { opacity: 1; top: 60%; left: 5%; }\n            100% { opacity: 1; top: 75%; left: 85%; }\n        }\n        \n        @keyframes ball-extra3-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 45%; left: 10%; }\n            62.5% { opacity: 1; top: 45%; left: 10%; }\n            62.6% { opacity: 1; top: 45%; left: 10%; }\n            75% { opacity: 1; top: 70%; left: 75%; }\n            87.5% { opacity: 1; top: 25%; left: 95%; }\n            100% { opacity: 1; top: 45%; left: 10%; }\n        }\n        \n        @keyframes ball-extra4-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 85%; left: 45%; }\n            62.5% { opacity: 1; top: 85%; left: 45%; }\n            62.6% { opacity: 1; top: 85%; left: 45%; }\n            75% { opacity: 1; top: 35%; left: 25%; }\n            87.5% { opacity: 1; top: 65%; left: 90%; }\n            100% { opacity: 1; top: 85%; left: 45%; }\n        }\n        \n        @keyframes ball-extra5-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 15%; left: 70%; }\n            62.5% { opacity: 1; top: 15%; left: 70%; }\n            62.6% { opacity: 1; top: 15%; left: 70%; }\n            75% { opacity: 1; top: 90%; left: 60%; }\n            87.5% { opacity: 1; top: 45%; left: 5%; }\n            100% { opacity: 1; top: 15%; left: 70%; }\n        }\n        \n        @keyframes ball-extra6-anim {\n            0% { opacity: 0; top: 50%; left: 50%; }\n            12.5% { opacity: 1; top: 30%; left: 90%; }\n            62.5% { opacity: 1; top: 30%; left: 90%; }\n            62.6% { opacity: 1; top: 30%; left: 90%; }\n            75% { opacity: 1; top: 75%; left: 10%; }\n            87.5% { opacity: 1; top: 50%; left: 70%; }\n            100% { opacity: 1; top: 30%; left: 90%; }\n        }\n        \n        /* 响应式设计 */\n        @media (max-width: 768px) {\n            .text-ball { width: 8px; height: 8px; }\n            body { background-size: 20px 20px; }\n        }\n        \n        @media (max-width: 480px) {\n            .text-ball { width: 6px; height: 6px; }\n            body { background-size: 15px 15px; }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"scene\">\n        <!-- CHUN 第一个 -->\n        <div class=\"text-ball ball-c1\"></div>\n        <div class=\"text-ball ball-h1\"></div>\n        <div class=\"text-ball ball-u1\"></div>\n        <div class=\"text-ball ball-n1\"></div>\n        \n        <!-- CHUN 第二个 -->\n        <div class=\"text-ball ball-c2\"></div>\n        <div class=\"text-ball ball-h2\"></div>\n        <div class=\"text-ball ball-u2\"></div>\n        <div class=\"text-ball ball-n2\"></div>\n        \n        <!-- STUDIO -->\n        <div class=\"text-ball ball-s\"></div>\n        <div class=\"text-ball ball-t\"></div>\n        <div class=\"text-ball ball-u3\"></div>\n        <div class=\"text-ball ball-d\"></div>\n        <div class=\"text-ball ball-i\"></div>\n        <div class=\"text-ball ball-o\"></div>\n        \n        <!-- 额外装饰光球 -->\n        <div class=\"text-ball ball-extra1\"></div>\n        <div class=\"text-ball ball-extra2\"></div>\n        <div class=\"text-ball ball-extra3\"></div>\n        <div class=\"text-ball ball-extra4\"